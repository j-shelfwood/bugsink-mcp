{
  "name": "bugsink-mcp",
  "version": "0.1.0",
  "description": "MCP server for interacting with Bugsink error tracking via LLMs",
  "author": "Shelfwood",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "bugsink-mcp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "bugsink",
    "sentry",
    "error-tracking",
    "llm",
    "ai",
    "claude",
    "cursor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/j-shelfwood/bugsink-mcp.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "peerDependencies": {
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0",
    "zod": "^3.25.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
